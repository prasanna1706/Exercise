vex.defaultOptions.className = 'vex-theme-plain';

(function ($) {
	$.fn.prompts = function () {
		var $form = $(this);

		$form.find('.prompt--text, .prompt--label, .prompt--select, .prompt--date, .prompt--option').on('focus mouseover', function (event) {
			$form.find('small[role="tooltip"]').removeClass('show--tooltip');
			$(event.target).closest('.prompt--text, .prompt--label, .prompt--select, .prompt--date, .prompt--option').find('small[role="tooltip"]').addClass('show--tooltip');
		});

		$form.find('.prompt--text, .prompt--label, .prompt--select, .prompt--date, .prompt--option').on('blur mouseout', function (event) {
			$form.find('small[role="tooltip"]').removeClass('show--tooltip');
		});
	}
}(jQuery));

$(function() {
    $('.modal--trigger').on('click', function() {
        vex.open({
            unsafeContent: $('#' + $(this).data('modal-id')).html(),
            contentClassName: 'dialog--content--' + $(this).data('modal-id'),
            afterOpen: function() {
                $('.datepicker-common').pickadate({
            		weekdaysShort: ['Su', 'Mo', 'Tu', 'We', 'Th', 'Fr', 'Sa'],
              		labelMonthNext: 'Go to the next month',
            		labelMonthPrev: 'Go to the previous month',
            		labelMonthSelect: 'Pick a month from the dropdown',
            		labelYearSelect: 'Pick a year from the dropdown',
            		selectMonths: true,
            		selectYears: 70
                });
                
                initNumberFormat();
            }
        });
    });
});

// ==========================================================================
// Put commas in numbers
// ==========================================================================
$(function() {
    initNumberFormat();
});

function initNumberFormat() {
    $(function () {
        $('input.number').val(function(index, value) {
            return formatNumber(value);
          });

        $('input.number').keyup(function(event) {
          // skip for arrow keys
          if(event.which >= 37 && event.which <= 40) return;
          // format number
          $(this).val(function(index, value) {
            return formatNumber(value);
          });
        });    
    });
}

function formatNumber(num) {
        var n1, n2;
        num = num.replace(/\D/g, "");
        num = num + '' || '';
        // works for integer and floating as well
        n1 = num.split('.');
        n2 = n1[1] || null;
        n1 = n1[0].replace(/(\d)(?=(\d\d)+\d$)/g, "$1,");
        num = n2 ? n1 + '.' + n2 : n1;
        return num;
}    

